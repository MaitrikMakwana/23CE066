#include<stdio.h>
#include<conio.h>
int main()
{ int a,b,c;

printf("enter the number a: ");
scanf("%d",&a);
printf("enter the number b: ");
scanf("%d",&b);
c=a;
a=b;
b=c;
printf("value of a after swapping:%d\n",a);
printf("value of b after swapping;%d\n",b);
printf("Maitrik 23ce066");

return 0;
}
