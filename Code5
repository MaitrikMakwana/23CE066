#include<stdio.h>
int main()
{
    int i,j,n;
    scanf("%d",&n);
    for(i=1;i<=n;i++)
    {
        for(j=1;j<=i;j++)
        {
            if(j%2==0)
            {printf("0");}
            else
            {printf("1");}
            
        }
         printf("\n");
    } 
    printf("this program is done by 23ce066-Maitrik");
return 0;
}       
